import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc =new Scanner(System.in);

        int sum = sc.nextInt();
        int kind = sc.nextInt();
        int result = 0;

        for(int i=0; i<kind; i++) {
            int a = sc.nextInt();
            int b = sc.nextInt();
            
            result += a*b;
        }
        
        if (sum==result) {
            System.out.print("Yes");
        } else {
            System.out.print("No");
        }
        sc.close();
    }
}
